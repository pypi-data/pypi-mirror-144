#!/bin/sh /etc/rc.common
USE_PROCD=1
START=95
STOP=01
start_service() {
    procd_open_instance
    procd_set_param command /usr/bin/python3 /tmp/unifi_respondd/respond.py
    procd_set_param stdout 1
    procd_set_param stderr 1
    procd_set_param env UNIFI_RESPONDD_CONFIG_FILE=/tmp/unifi_respondd/unifi_respondd.yaml
    procd_close_instance
}