# Ignore vim tmp files
*.swp
*.swo
.netrwhist

# Ignore some Python-related stuff
__pycache__/
*.egg
*.egg-info
.tox/
_version.py

# Virtual environment
env/
venv/
virt/

# Build folders
build/
BUILD/
dist/

# Git and mergetool stuff
*.orig

# Ignore qwilfish databases log files (legacy)
qwilfish_log*.txt
qwilfish_results_db*.txt
qwilfish*.db

# Ignore code generated by protobufs, but keep the containing folder
!src/qwilfish/generated/
src/qwilfish/generated/*
!src/qwilfish/generated/__init__.py

# Ignore new conf files except for what is needed for the default conf
!src/qwilfish/configuration/
src/qwilfish/configuration/*
!src/qwilfish/configuration/__init__.py
!src/qwilfish/configuration/default_session_conf.yaml
!src/qwilfish/configuration/default_log_conf.yaml

# Ignore new plugins
!src/qwilfish/plugins/
src/qwilfish/plugins/*
!src/qwilfish/plugins/__init__.py
!src/qwilfish/plugins/coap_grammar.py
!src/qwilfish/plugins/lldp_grammar.py
!src/qwilfish/plugins/raw_socket_courier.py
!src/qwilfish/plugins/udp_socket_courier.py
