{
    "title": "MutableAI Settings",
    "description": "MutableAI Settings",
    "jupyter.lab.menus": {
        "main": [
            {
                "id": "jupyter-mutableai-settings",
                "label": "MutableAI Settings",
                "rank": 80,
                "items": [
                    {
                        "command": "jupyterlab_mutableai/settings:toggle-flag"
                    },
                    {
                        "command": "jupyterlab_mutableai/settings:update-settings"
                    }
                ]
            }
        ]
    },
    "properties": {
        "flag": {
            "type": "boolean",
            "title": "Autocomplete Flag",
            "description": "This controls whether or not autocomplete is activated.",
            "default": true
        },
        "apiKey": {
            "type": "string",
            "title": "API key",
            "description": "This is the api key to call the endpoints.",
            "default": "pleaseInsertKey"
        },
        "autocompleteDomain": {
            "type": "string",
            "title": "Autocomplete Domain",
            "description": "Used to construct url to call autocomplete endpoint",
            "default": "api.mutableai.com"
        },
        "transformDomain": {
            "type": "string",
            "title": "Transfer Domain",
            "description": "Used to construct url to call transform endpoint",
            "default": "api.mutableai.com"
        }
    },
    "additionalProperties": false,
    "type": "object"
}